{

  "manifest_version": 2,
  "name": "Google Meet Attendance",
  "version": "0.1",

  "description": "A tool that will mark the attendance of everyone present in the Google Meet and will serve it to you in a text file format.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "192": "icons/icon192.png",
    "256": "icons/icon256.png",
    "384": "icons/icon384.png",
    "512": "icons/icon512.png",
    "768": "icons/icon768.png"
  },

  "page_action": {},

  "author": "Rajat Kiran Patil",

  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },

  "permissions": [
    "declarativeContent",
    "tabs",
    "https://meet.google.com/*"
  ]

}
